    <global>
        <models>
            <warehouse>
                <class>Trenza_Warehouse_Model</class>
                <resourceModel>warehouse_mysql4</resourceModel>
            </warehouse>
			
            <warehouse_mysql4>
                <class>Trenza_Warehouse_Model_Mysql4</class>
                <entities>
                    <warehouse>
                        <table>warehouse</table>
                    </warehouse>
                </entities>
            </warehouse_mysql4>
			
        </models>
		
        <resources>
            <warehouse_setup>
                <setup>
                    <module>Trenza_Warehouse</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </warehouse_setup>
            <warehouse_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </warehouse_write>
            <warehouse_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </warehouse_read>
        </resources>
		
		<helpers>
            <warehouse>
                <class>Trenza_Warehouse_Helper</class>
            </warehouse>
       </helpers>
	   
	    <blocks>
            <warehouse>
                <class>Trenza_Warehouse_Block</class>
            </warehouse>
			
			<adminhtml>
            <rewrite>
                <sales_order_grid>Trenza_Warehouse_Block_Sales_Order_Grid</sales_order_grid>
                <catalog_product_grid>Trenza_Warehouse_Block_Catalog_Product_Grid</catalog_product_grid>
            </rewrite>
			</adminhtml>
		
        </blocks>

    </global>

</config>